#include<iostream>
using namespace std;

int main(){
	char a[2][600];
	int n,j=0;
	cin>>n;
	for(int i=0;i<2;i++){
		cin>>a[i];	
	}
	int i;
	for(i=0;i<n;i++){
		if(a[0][i]=='#'){
			j=0;break;
		}
		if(a[1][i]=='#'){
			j=1;break;
		}
	}
	cout<<"i="<<i<<'\n';	
	cout<<"j="<<j<<'\n';
	int cond_check=1;
	while(i<n-1){
	cout<<"i="<<i<<'\n';	
	cout<<"j="<<j<<'\n';	
		if(j==0){
			if((a[j+1][i]=='.'&&a[j][i+1]=='.')||(a[j+1][i]=='#'&&a[j][i+1]=='.')) break;
			cout<<"j is 0\n";
			if(a[j][i+1]=='#'){
				cout<<"next i cell has #\n";
				if((a[j+1][i]=='#')&&(a[j][i-1]=='#')) {cout<<"down cell has # \nno\n";cond_check=0;break;}
				else i++;
			}
			else if(a[j+1][i]=='#') {j=1;cout<<"down cell has #\n";}
			else {cout<<"no last case\n"; cond_check=0;break;}
		}
		else if(j==1){
			cout<<"j is 1\n";
			if((a[j-1][i]=='.'&&a[j][i+1]=='.')||(a[j-1][i]=='#'&&a[j][i+1]=='.')) break;
			if(a[j][i+1]=='#'){
				cout<<"Next cell has # \n";
				if((a[j-1][i]=='#')&&(a[j][i-1]=='#')) {cout<<"Up cell has # and previous cell has a # \nno \n"; cond_check=0; break;}
				else i++;
			}
			else if(a[j-1][i]=='#') {j=0;cout<<"Up cell has # \n";}
			else {cout<<"no\n"; cond_check=0; break;}
		}
	}
	int f=0;
	if(cond_check==1) {
	for (int m=i;m<n;m++){
		if(a[0][i]=='#'||a[1][j]=='#')
			{cout<<"no\n"; f=1;break;	}
		
	}
	}
	if(!f)
	cout<<"yes\n";
}		
